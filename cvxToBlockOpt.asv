function x = cvxToBlockOpt(He_fzf, Fbb_zf, k_cluster)

[m,n] = size(Fbb_zf);
cvx_begin
        variable x(m,n)
        minimize(norm(He_fzf*x-Fbb_zf,'fro'))
        subject to 
                    for col = 1:Num_users
                        for row = 1:Num_users
                            if abs(row-col)>=(Num_users/k_cluster)
                            Fbb_BDMA(col,row) = 0;
                            end
                        end
                    end
cvx_end